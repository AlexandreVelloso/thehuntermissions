{
    "$id": "loginResponse",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Login response",
    "definitions": {
        "login": {
            "type": "object",
            "properties": {
                "user": {
                    "type": "object",
                    "properties": {
                        "username": {
                            "type": "string"
                        },
                        "email": {
                            "type": "string"
                        }
                    }
                },
                "access_token": {
                    "type": "string"
                },
                "refresh_token": {
                    "type": "string"
                }
            }
        }
    }
}